  </section>

  <!--=============== PORTFOLIO ===============-->
  <section class="section section__height container" id="portfolio">
    <%- include("title.ejs", {sectionTitle : "Presences"}) %>
    <%-include("./main/absenteeism.ejs")%>
  </section>

  <!--=============== SKILLS ===============-->
  <section class="section section__height container" id="skills">
    <%- include("title.ejs", {sectionTitle : "Calendrier des Examens"}) %>
    <%-include("./main/examsCalendar.ejs")%>
  </section>

  <!--=============== CONTACT ===============-->
  <section class="section section__height container" id="contact">
    <%- include("title.ejs", {sectionTitle : "contact"}) %>
  </section>
</main>
